{
  "extends": [
    "airbnb",
    "plugin:react/recommended"
  ],
  "plugins": [
    "react"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "indent": [2, 4],
    "comma-dangle": [0, "never"],
    "react/jsx-indent-props": 0,
    "react/jsx-closing-bracket-location": 0
  }
}
